<div class="container">
  <div>
  <p class="button10b">
    <%= @artist.name %>
    <% if @artist.album_id.present?  %>
    Projects: <%= @artist.album_id %>
    <% end %>
    </p>


    <%= image_tag "#{@artist.picture_url}" %>
    <%= image_tag "#{@artist.picture_url2}" %>

  </div>

</div>

  <div class="container">
      <ul class="feedxxx">
          <% @artist.songs.order(release_date: :desc).each do |song| %>
            <li>
              <a href="/songs/<%= song.id %>">
                <p class="artist-feed">
                  <%= song.artist.name %>
                  <% if song.release_date.present? %>
                    <span><%= time_ago_in_words(song.release_date) %></span>
                  <% end %>
                </p>
                <table>
                  <td style="padding-right:25px">
                    <%= song.title %>
                  <td style="padding-right:25px">
                    <% if song.features.present? %>
                      Feat.
                    <%= song.features %>
                  </td>
                  </td>
                  <td style="padding-right:25px">
                      <% if song.album_title.present? %>
                      Album: <%= song.album_title %>
                    <% end %>
                    <% end %>
                  </td>
                </table>
              </a>
            </li>
          <% end %>
        </ul>
    </div>

