<div class="container">
  <p id="notice"><%= notice %></p>
  <% if current_user.subscriptions.present? %>
  <h1 class="button10b">My Subscriptions</h1>
  <% end %>
  <%= render 'my_subs' %>
</div>

<div class="container">
  <h1 class="button10b">Select More Artists to Follow</h1>
  <a class="button10c" href="mailto:contact.ntouchmusic@gmail.com">Your FAVORITE ARTIST not listed? Click here to let us know, ( contact.ntouchmusic@gmail.com ) then give us about a week.</a></p>
  <div class="row">
    <div class="col-xs-12">
      <%= form_tag subscriptions_path, method: "get" do %>
        <%= search_field_tag "search",
              params[:search],
              placeholder: "Search Artist",
              class: "form-control" %>
      <% end %>
    </div>
  </div>
</div>

<br>

<div class="container">
  <%= render 'new_subs' %>
</div>
