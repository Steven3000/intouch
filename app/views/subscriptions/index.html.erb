<div class="container">
  <p id="notice"><%= notice %></p>
  <h1>My Subscriptions</h1>
  <%= render 'my_subs' %>
</div>

<div class="container">
  <h1>Pick More Artists to Follow</h1>
  <div class="row">
    <div class="col-xs-12">
      <%= form_tag subscriptions_path, method: "get" do %>
        <%= search_field_tag "search",
              params[:search],
              placeholder: "Search Artist",
              class: "form-control" %>
      <% end %>
    </div>
  </div>
</div>

<br>

<div class="container">
  <%= render 'new_subs' %>
</div>
