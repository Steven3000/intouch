<% @subscriptions.each do |subscription| %>

  <div class="col-xs-12 col-lg-3 isotope-item box-subs">
    <div class="panel panel-default">
      <div class="panel-body">
      <%= image_tag subscription.artist.avatar.url(:medium) %>
        <dl>
          <br>
          <dd class="btn-md button12b"><%= subscription.artist.name %></dd>



          <dd><%= link_to 'Unsubscribe', subscription_path(subscription), :method => :delete, :class => "btn btn-danger", :style => "width:100%;" %></dd>

           <dd class="btn-md button12b"><%= link_to "Checkout Artist Profile", artist_path(subscription.artist.id) %></dd>
        </dl>
      </div>
    </div>
  </div>

<% end %>
