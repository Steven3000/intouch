<% @subscriptions.each do |subscription| %>

  <div class="col-xs-12 col-lg-3 isotope-item">
    <div class="panel panel-default">
      <div class="panel-body box-subs">
      <%= image_tag subscription.artist.avatar.url(:medium) %>
        <dl>
          <br>
          <dd class="button12b"><%= subscription.artist.name %><br/><span style="font-size:8px"><%= link_to "(Click here for more...)", artist_path(subscription.artist.id) %></span></dd>
          <dd><%= link_to 'Unsubscribe', subscription_path(subscription), :method => :delete, :class => "btn btn-danger", :style => "width:100%;" %></dd>
        </dl>
      </div>
    </div>
  </div>

<% end %>
