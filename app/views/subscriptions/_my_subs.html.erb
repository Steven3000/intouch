<% @subscriptions.each do |subscription| %>
  <div class="col-xs-12 col-lg-3 isotope-item box-subs">
    <div class="panel panel-default">
      <div class="panel-body">
        <%= link_to artist_path(subscription.artist.id) do %>
        <%= image_tag subscription.artist.avatar.url(:medium) %>
        <% end %>

        <dl>
          <dd class="btn-md panel-button-big navv-font">
            <%= subscription.artist.name %>
          </dd>
          <dd><%= link_to 'Unsubscribe', subscription_path(subscription), :method => :delete, :class => "btn btn-danger navv-font", :style => "width:100%;" %>
          </dd>
        </dl>
      </div>
    </div>
  </div>

<% end %>
