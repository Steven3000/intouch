<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=Edge,chrome=1">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title><%= content_for?(:title) ? yield(:title) : "IndyChicago" %></title>
  <%= csrf_meta_tags %>
  <meta name="author" content="Steven Callen aka Mulatto Patriot">

      <!-- Le HTML5 shim, for IE6-8 support of HTML elements -->
  <!--[if lt IE 9]>
    <script src="http://html5shim.googlecode.com/svn/trunk/html5.js" type="text/javascript"></script>
  <![endif]-->

  <%= stylesheet_link_tag "feed_layout", media: "all" %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>

  <%= yield(:page_stylesheet) if content_for?(:page_stylesheet) %>
</head>

<body>
 <style type="text/css">
    body {
    background-image: url("https://s3-us-west-2.amazonaws.com/ntouchimages/admin/chicago-chopped.jpg");
    background-repeat: no-repeat;
    background-size: cover;
    }
  </style>

 <div class="wrapper">
   <!--=== Header v6 ===-->
    <div class="header-v6 header-border-bottom header-sticky">
        <!-- Navbar -->
        <div class="navbar mega-menu" role="navigation">
            <div class="container">
                <!-- Brand and toggle get grouped for better mobile display -->
                <div class="menu-container">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-responsive-collapse">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>

                    <!-- Navbar Brand -->
                    <div class="navbar-brand">
                        <a href="index.html">
                            <!-- <img class="default-logo" src="#" alt="Logo">
                            <img class="shrink-logo" src="#" alt="Logo"> -->
                        </a>
                    </div>
                    <!-- ENd Navbar Brand -->

                    <!-- Header Inner Right -->
                    <div class="header-inner-right">

                    </div>
                </div>

                <!-- Header Inner Right -->

                <!-- Collect the nav links, forms, and other content for toggling -->
                <div class="collapse navbar-collapse navbar-responsive-collapse">
                    <div class="menu-container">
                        <ul class="nav navbar-nav">
                            <!-- Home -->
                            <li class="header-nav-main">
                                <%= link_to_if (request.path != "/home.html"),
                                    "HOME", root_path %>
                            </li>
                            <li class="header-nav-main">
                            <%= link_to_if (request.path != subscriptions_path),
                                 "Follow Artists", subscriptions_path %>
                            </li>
                            <li class="header-nav-main">
                                <%= link_to_if (request.path != songs_path),
                                   "Listen to Songs", "/songs" %>
                            </li>
                            <li class="header-nav-main">
                                <%= link_to_if (request.path != albums_path),
                                   "Listen to Albums", "/albums" %>
                            </li>


                            <% if user_signed_in? %>
                            <li class="header-nav-main"><%= link_to "My Profile", edit_user_registration_path %></li>
                            <li class="header-nav-main"><%= link_to "Sign Out", destroy_user_session_path, :method => :delete %></li>
                            <% else %>
                            <li class="header-nav-main"><%= link_to "Sign Up", new_user_registration_path %></li>
                            <li class="header-nav-main"><%= link_to "Sign In", new_user_session_path %></li>
                            <% end %>
                        </ul>
                    </div>
                </div><!--/navbar-collapse-->
            </div>
        </div>
        <!-- End Navbar -->
    </div>
    <!--=== End Header v6 ===-->

